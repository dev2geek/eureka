body 
  background-color #58a8c1

header
  h1
    background-color #303030
    width 100%
    height 10%
    margin 0

    text-align center
    position relative
    a 
      color #fff
      text-decoration none
      line-height 50px

      &:visited 
        color #fff
        text-decoration none

      &.icon-back
        position absolute
        top 10px
        left 0
      &.icon-add
        position absolute
        top 10px
        right 20px

.main 
    height 385px
    overflow auto
    background-color #fff 
    margin 25px 25px 


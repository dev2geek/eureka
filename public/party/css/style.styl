html
body
    height 100%
    color #1D1D1D
.bar
    height 56px
    color #FFF
    background #303030
    h1
        font-size 24px
        font-weight normal
        line-height @height
        color #FFF
        text-align center
    .back
        absolute left 0 top 0;
        display block
        width @height
        height @height
    .btn
        absolute right 0 top 0
        display block
        width @height
        height @height
.panel
    padding 20px
.party-item
    padding-bottom 50px
    margin-bottom 10px
    background #FFF
    border-radius 5px
    .party-info
        background #CDEEFA
        border-top-left-radius 5px
        border-top-right-radius 5px
        padding 10px
        line-height 1.8
        h1
            padding 5px 0
            color #333
            font-size 20px
            a
                color #141110
                margin-right 10px
                font-size 14px
            .session-name
                text-decoration none
                font-size 16px
